  "author": "PvdZ <pvdz@github>",
  "dependencies": {
    "del-cli": "^3.0.0",
    "faker": "^4.1.0",
    "gatsby": "v1",
    "gatsby-image": "v1",
    "gatsby-plugin-feed": "v1",
    "gatsby-plugin-google-analytics": "v1",
    "gatsby-plugin-manifest": "v1",
    "gatsby-plugin-offline": "v1",
    "gatsby-plugin-react-helmet": "v1",
    "gatsby-plugin-sharp": "v1",
    "gatsby-plugin-typography": "v1",
    "gatsby-remark-copy-linked-files": "v1",
    "gatsby-remark-images": "v1",
    "gatsby-remark-prismjs": "v1",
    "gatsby-remark-responsive-iframe": "v1",
    "gatsby-remark-smartypants": "v1",
    "gatsby-source-filesystem": "v1",
    "gatsby-transformer-remark": "v1",
    "gatsby-transformer-sharp": "v1",
